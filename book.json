{
  "title": "SpringCloud Note",
  "author": "victor",
  "description": "记录SpringCloud的笔记和课件",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "Home": "https://www.baidu.com"
    }
  },
  "plugins": [
    "hide-element",
    "chapter-fold",
    "expandable-chapters",
    "favicon",
    "lightbox",
    "splitter",
    "-lunr",
    "pageview-count",
    "ancre-navigation"
  ],
  "pluginsConfig": {
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "page-treeview": {
      "copyright": "Copyright &#169; aleen42",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "favicon": {
      "shortcut": "./img/avator.png.icon.ico",
      "bookmark": "./img/avator.png.icon.ico"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    }
  }
}

